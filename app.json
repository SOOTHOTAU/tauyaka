{
  "expo": {
    "name": "Yaka",
    "slug": "yaka",
    "scheme": "yaka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": false,
    "plugins": [
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "android": {
      "package": "com.soothotau.yaka",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "bundleIdentifier": "com.soothotau.yaka",
      "supportsTablet": true
    },
    "extra": {
      "eas": {
        "projectId": "e06bd5eb-a94d-4363-912f-273025f1e233"
      }
    }
  }
}
