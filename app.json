{
  "expo": {
    "name": "Yaka",
    "slug": "yaka",
    "version": "1.0.0",
    "scheme": "yaka",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/00000000-0000-0000-0000-000000000000",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soothotau.yaka"
    },
    "android": {
      "package": "com.soothotau.yaka",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
            /* You can optionally set these if needed:
               "minSdkVersion": 23,
               "compileSdkVersion": 35,
               "targetSdkVersion": 35,
               "kotlinVersion": "1.9.24"
            */
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
      /* add other plugins here if you actually use them, e.g.
         ["expo-router"],
         ["expo-localization"],
         etc.
      */
    ],
    "extra": {
      "eas": {
        "projectId": "PUT-YOUR-EAS-PROJECT-ID-HERE"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
